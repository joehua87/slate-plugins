    "build:json": "cpx src/**/*.json lib",
    "prepublish": "npm run build",
    "test": "mocha --require babel-register --require testHelper.js src/**/__test__/*.test.js",
    "test:watch": "mocha --watch --require babel-register --require testHelper.js src/**/__test__/*.test.js"
